 <!-- ########################################################################################################## 

                                         MAIN LAYOUT PAGE FOR QUIZZES
                                          
 ########################################################################################################### -->

<div class="container">
<br>
<br>
<br>
  <div class="flash">
    <% flash.each do |name, msg| %>
        <%if correct_flash_name(name)=='info' || correct_flash_name(name)=='alert'%>
        <div class="alert alert-<%= correct_flash_name(name) %>" role="alert">
          <%= msg %>
        </div>
            <%end%>
    <% end %>
  </div>
 
 
</div>
<%= yield %>

